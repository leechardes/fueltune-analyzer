{
  "dashboard": {
    "id": null,
    "title": "FuelTune Streamlit - Application Dashboard",
    "tags": ["fueltune", "streamlit", "application"],
    "style": "dark",
    "timezone": "browser",
    "editable": true,
    "graphTooltip": 0,
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    },
    "refresh": "30s",
    "version": 1,
    "panels": [
      {
        "id": 1,
        "title": "Application Status",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "up{job=\"fueltune-app\"}",
            "legendFormat": "{{ pod }}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "mappings": [
              {"options": {"0": {"text": "DOWN"}}, "type": "value"},
              {"options": {"1": {"text": "UP"}}, "type": "value"}
            ],
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "green", "value": 1}
              ]
            }
          }
        },
        "options": {
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          }
        }
      },
      {
        "id": 2,
        "title": "Request Rate",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 0},
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{job=\"fueltune-app\"}[5m]))",
            "legendFormat": "Requests/sec",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "reqps",
            "decimals": 2
          }
        }
      },
      {
        "id": 3,
        "title": "Response Time (95th percentile)",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{job=\"fueltune-app\"}[5m]))",
            "legendFormat": "95th percentile",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "unit": "s",
            "decimals": 3,
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 3}
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Error Rate",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 0},
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{job=\"fueltune-app\",status=~\"5..\"}[5m])) / sum(rate(http_requests_total{job=\"fueltune-app\"}[5m])) * 100",
            "legendFormat": "Error Rate",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "unit": "percent",
            "decimals": 2,
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 5}
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "HTTP Requests Over Time",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4},
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{job=\"fueltune-app\"}[5m])) by (status)",
            "legendFormat": "{{ status }}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {"label": "Requests/sec", "unit": "reqps"},
          {"show": false}
        ],
        "legend": {
          "show": true,
          "values": true,
          "current": true
        }
      },
      {
        "id": 6,
        "title": "Response Time Distribution",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket{job=\"fueltune-app\"}[5m]))",
            "legendFormat": "50th percentile",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{job=\"fueltune-app\"}[5m]))",
            "legendFormat": "95th percentile",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(http_request_duration_seconds_bucket{job=\"fueltune-app\"}[5m]))",
            "legendFormat": "99th percentile",
            "refId": "C"
          }
        ],
        "yAxes": [
          {"label": "Response Time", "unit": "s"},
          {"show": false}
        ]
      },
      {
        "id": 7,
        "title": "CPU Usage",
        "type": "graph",
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 12},
        "targets": [
          {
            "expr": "rate(container_cpu_usage_seconds_total{pod=~\"fueltune-app-.*\"}[5m]) * 100",
            "legendFormat": "{{ pod }}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {"label": "CPU %", "unit": "percent", "max": 100},
          {"show": false}
        ]
      },
      {
        "id": 8,
        "title": "Memory Usage",
        "type": "graph",
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 12},
        "targets": [
          {
            "expr": "container_memory_working_set_bytes{pod=~\"fueltune-app-.*\"} / 1024 / 1024",
            "legendFormat": "{{ pod }}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {"label": "Memory", "unit": "MB"},
          {"show": false}
        ]
      },
      {
        "id": 9,
        "title": "Pod Restarts",
        "type": "graph",
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 12},
        "targets": [
          {
            "expr": "kube_pod_container_status_restarts_total{namespace=\"fueltune\",pod=~\"fueltune-app-.*\"}",
            "legendFormat": "{{ pod }}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {"label": "Restarts", "unit": "short"},
          {"show": false}
        ]
      },
      {
        "id": 10,
        "title": "Database Connections",
        "type": "graph",
        "gridPos": {"h": 6, "w": 12, "x": 0, "y": 20},
        "targets": [
          {
            "expr": "pg_stat_database_numbackends{datname=\"fueltune_prod\"}",
            "legendFormat": "Active Connections",
            "refId": "A"
          }
        ],
        "yAxes": [
          {"label": "Connections", "unit": "short"},
          {"show": false}
        ]
      },
      {
        "id": 11,
        "title": "Redis Operations",
        "type": "graph",
        "gridPos": {"h": 6, "w": 12, "x": 12, "y": 20},
        "targets": [
          {
            "expr": "rate(redis_commands_processed_total[5m])",
            "legendFormat": "Commands/sec",
            "refId": "A"
          }
        ],
        "yAxes": [
          {"label": "Operations/sec", "unit": "ops"},
          {"show": false}
        ]
      },
      {
        "id": 12,
        "title": "File Processing Rate",
        "type": "graph",
        "gridPos": {"h": 6, "w": 12, "x": 0, "y": 26},
        "targets": [
          {
            "expr": "rate(fueltune_data_files_processed_total[5m])",
            "legendFormat": "Files Processed/sec",
            "refId": "A"
          },
          {
            "expr": "rate(fueltune_data_processing_errors_total[5m])",
            "legendFormat": "Processing Errors/sec",
            "refId": "B"
          }
        ],
        "yAxes": [
          {"label": "Files/sec", "unit": "short"},
          {"show": false}
        ]
      },
      {
        "id": 13,
        "title": "Data Volume",
        "type": "graph",
        "gridPos": {"h": 6, "w": 12, "x": 12, "y": 26},
        "targets": [
          {
            "expr": "fueltune_upload_file_size_bytes",
            "legendFormat": "Upload Size",
            "refId": "A"
          },
          {
            "expr": "fueltune_processed_data_size_bytes",
            "legendFormat": "Processed Size",
            "refId": "B"
          }
        ],
        "yAxes": [
          {"label": "Bytes", "unit": "bytes"},
          {"show": false}
        ]
      },
      {
        "id": 14,
        "title": "Ingress Request Rate",
        "type": "graph",
        "gridPos": {"h": 6, "w": 24, "x": 0, "y": 32},
        "targets": [
          {
            "expr": "sum(rate(nginx_ingress_controller_requests{ingress=\"fueltune-ingress\"}[5m])) by (status)",
            "legendFormat": "Status {{ status }}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {"label": "Requests/sec", "unit": "reqps"},
          {"show": false}
        ]
      },
      {
        "id": 15,
        "title": "Top URLs by Request Count",
        "type": "table",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 38},
        "targets": [
          {
            "expr": "topk(10, sum(rate(http_requests_total{job=\"fueltune-app\"}[5m])) by (path))",
            "format": "table",
            "refId": "A"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {"Time": true},
              "renameByName": {"path": "URL Path", "Value": "Requests/sec"}
            }
          }
        ]
      },
      {
        "id": 16,
        "title": "Alert Status",
        "type": "table",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 38},
        "targets": [
          {
            "expr": "ALERTS{job=\"fueltune-app\"}",
            "format": "table",
            "refId": "A"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {"Time": true, "__name__": true},
              "renameByName": {"alertname": "Alert", "alertstate": "State", "severity": "Severity"}
            }
          }
        ]
      }
    ],
    "templating": {
      "list": [
        {
          "name": "namespace",
          "type": "query",
          "query": "label_values(up{job=\"fueltune-app\"}, namespace)",
          "refresh": 1,
          "includeAll": false,
          "multi": false,
          "current": {"text": "fueltune", "value": "fueltune"}
        },
        {
          "name": "pod",
          "type": "query",
          "query": "label_values(up{job=\"fueltune-app\", namespace=\"$namespace\"}, pod)",
          "refresh": 1,
          "includeAll": true,
          "multi": true
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "Prometheus",
          "enable": true,
          "expr": "changes(kube_deployment_status_replicas{namespace=\"fueltune\"}[5m]) > 0",
          "titleFormat": "Deployment Updated",
          "textFormat": "{{ deployment }} replicas changed"
        },
        {
          "name": "Alerts",
          "datasource": "Prometheus",
          "enable": true,
          "expr": "ALERTS{alertname!=\"\"}",
          "titleFormat": "{{ alertname }}",
          "textFormat": "{{ summary }}"
        }
      ]
    }
  },
  "overwrite": true,
  "inputs": [
    {
      "name": "DS_PROMETHEUS",
      "type": "datasource",
      "pluginId": "prometheus",
      "value": "Prometheus"
    }
  ],
  "requires": [
    {
      "type": "grafana",
      "id": "grafana",
      "name": "Grafana",
      "version": "8.0.0"
    },
    {
      "type": "panel",
      "id": "graph",
      "name": "Graph",
      "version": ""
    },
    {
      "type": "panel",
      "id": "stat",
      "name": "Stat",
      "version": ""
    },
    {
      "type": "panel",
      "id": "table",
      "name": "Table",
      "version": ""
    },
    {
      "type": "datasource",
      "id": "prometheus",
      "name": "Prometheus",
      "version": "1.0.0"
    }
  ]
}